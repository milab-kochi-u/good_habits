{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:prod": "node ./bin/www",
    "start:dev": "nodemon -e ejs,js,json ./bin/www",
    "start:sync": "browser-sync start --config bs-config.js",
    "css": "postcss public/stylesheets/tailwind.css -o public/stylesheets/style.css",
    "css-dev": "postcss public/stylesheets/tailwind.css -o public/stylesheets/style.css -w",
    "start": "npm-run-all --parallel start:prod css",
    "start-dev": "npm-run-all --parallel start:dev start:sync css-dev"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "axios": "^1.6.2",
    "child_process": "^1.0.2",
    "cookie-parser": "~1.4.4",
    "csv": "^6.2.0",
    "dayjs": "^1.11.3",
    "debug": "~2.6.9",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "sequelize": "^6.20.0",
    "sequelize-cli": "^6.4.1",
    "sqlite3": "^5.0.8",
    "tailwindcss": "^3.2.4",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "browser-sync": "^2.27.11",
    "nodemon": "^1.3.3",
    "npm-run-all": "^4.1.5"
  }
}
